<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0.2" class="note" default-locale="en-US">
  <info>
    <title>Texas Greenbook (15th ed.) — Table of Authorities (Grouped)</title>
    <title-short>Texas Greenbook 15th TOA (Grouped)</title-short>
    <id>http://www.zotero.org/styles/texas-greenbook-15th-toa-grouped</id>
    <link href="http://www.zotero.org/styles/texas-greenbook-15th-toa-grouped" rel="self"/>
    <link href="http://www.zotero.org/styles/texas-greenbook-15th-master" rel="template"/>
    <link href="https://www.txcourts.gov/rules-forms/rules-standards/" rel="documentation"/>
    <author><name>GPT-5 Pro</name></author>
    <category citation-format="note"/>
    <category field="law"/>
    <updated>2025-11-01T01:20:17Z</updated>
    <summary>TOA-style bibliography grouped by authority type for Texas practice: Cases → Constitution → Statutes → TAC → Court Rules → Session Laws → AG Opinions → Municipal → Secondary.</summary>
  </info>

  <locale xml:lang="en-US">
    <terms>
      <term name="section" form="symbol">§</term>
      <term name="article" form="short">art.</term>
      <term name="chapter" form="short">ch.</term>
      <term name="rule" form="short">R.</term>
    </terms>
  </locale>

  <!-- Rank macro to drive group ordering in the bibliography sort -->
  <macro name="toa-type-rank">
    <choose>
      <if type="legal_case">
        <text value="01"/>
      </if>
      <else-if type="legislation">
        <choose>
          <if variable="chapter-number">
            <text value="02"/>
          </if>
          <else-if variable="title">
            <text value="06"/>
          </else-if>
          <else>
            <text value="03"/>
          </else>
        </choose>
      </else-if>
      <else-if type="regulation">
        <choose>
          <if variable="volume">
            <text value="04"/>
          </if>
          <else-if variable="authority chapter-number" match="all">
            <text value="08"/>
          </else-if>
          <else>
            <text value="05"/>
          </else>
        </choose>
      </else-if>
      <else-if type="report">
        <text value="07"/>
      </else-if>
      <else>
        <text value="09"/>
      </else>
    </choose>
  </macro>

  <!-- Case formatting -->
  <macro name="case-name"><text variable="title" font-style="italic"/></macro>
  <macro name="reporter-print">
    <group delimiter=" ">
      <text variable="volume"/>
      <text variable="container-title"/>
      <text variable="page"/>
    </group>
  </macro>
  <macro name="parenthetical-docket">
    <choose>
      <if variable="number collection-number" match="any">
        <group delimiter=", ">
          <text variable="number" prefix="No. "/>
          <text variable="collection-number"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="reporter-wl">
    <text macro="parenthetical-docket"/>
  </macro>
  <macro name="pinpoint">
    <choose>
      <if variable="volume page container-title" match="all"><text variable="locator" prefix=", "/></if>
      <else-if variable="collection-number locator" match="all"><text variable="locator" prefix=", at *"/></else-if>
    </choose>
  </macro>

  <macro name="tex-labeled-locator">
    <choose>
      <if locator="section paragraph" match="any">
        <group delimiter=" "><label variable="locator" form="symbol"/><text variable="locator"/></group>
      </if>
      <else-if locator="rule regulation" match="any">
        <group delimiter=" "><label variable="locator" form="short"/><text variable="locator"/></group>
      </else-if>
      <else-if locator="article chapter part title volume" match="any">
        <group delimiter=" "><label variable="locator" form="short"/><text variable="locator"/></group>
      </else-if>
      <else>
        <text variable="locator"/>
      </else>
    </choose>
  </macro>

  <macro name="cross-reference-cue">
    <choose>
      <if variable="annote" match="none">
        <choose>
          <if variable="jurisdiction">
            <text value="See also"/>
          </if>
          <else>
            <text value="See"/>
          </else>
        </choose>
      </if>
      <else>
        <text variable="annote"/>
      </else>
    </choose>
  </macro>
  <macro name="parenthetical-procedural-note">
    <choose>
      <if variable="note annote" match="all">
      </if>
      <else-if variable="note">
        <text variable="note"/>
      </else-if>
    </choose>
  </macro>
  <macro name="parenthetical-procedural-status">
    <choose>
      <if variable="status note" match="any">
        <group delimiter=", ">
          <text variable="status"/>
          <choose>
            <if variable="status">
              <text macro="parenthetical-procedural-note"/>
            </if>
            <else-if variable="note">
              <choose>
                <if variable="references">
                </if>
                <else>
                  <text macro="parenthetical-procedural-note"/>
                </else>
              </choose>
            </else-if>
          </choose>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="court-and-date">
    <choose>
      <if variable="volume page container-title" match="all">
        <group prefix=" (" suffix=")">
          <text variable="authority"/>
          <date variable="issued" prefix=" ">
            <date-part name="year"/>
          </date>
          <text macro="parenthetical-procedural-status" prefix=", "/>
        </group>
      </if>
      <else>
        <group prefix=" (" suffix=")">
          <text variable="authority"/>
          <date variable="issued" prefix=" ">
            <date-part name="month" form="short" suffix=" "/>
            <date-part name="day" form="numeric" suffix=", "/>
            <date-part name="year"/>
          </date>
          <text macro="parenthetical-procedural-status" prefix=", "/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="parenthetical-slip-op">
    <choose>
      <if variable="genre medium" match="any">
        <group delimiter="">
          <text variable="genre" font-style="normal" prefix=" (" suffix=")"/>
          <text variable="medium" font-style="normal" prefix=" (" suffix=")"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="explanatory-parenthetical">
    <choose>
      <if variable="abstract">
        <text variable="abstract" prefix=" (" suffix=")"/>
      </if>
    </choose>
  </macro>
  <macro name="case-parenthetical-stack">
    <text macro="parenthetical-slip-op"/>
    <text macro="explanatory-parenthetical"/>
  </macro>
  <macro name="legal-case">
    <group delimiter=", ">
      <text macro="case-name"/>
      <choose>
        <if variable="volume page container-title" match="all"><text macro="reporter-print"/></if>
        <else><text macro="reporter-wl"/></else>
      </choose>
    </group>
    <text macro="pinpoint"/>
    <text macro="court-and-date"/>
    <text macro="case-parenthetical-stack"/>
  </macro>

  <!-- Statutes / Constitution / TAC / Session law / Rules / Municipal / AG -->
  <macro name="tex-code-section">
    <group delimiter=" ">
      <text variable="container-title"/>
      <choose>
        <if variable="chapter-number">
          <group delimiter=" ">
            <text variable="chapter-number"/>
            <choose>
              <if variable="locator">
                <text macro="tex-labeled-locator"/>
              </if>
              <else-if type="regulation">
                <group delimiter=" ">
                  <text term="section" form="symbol"/>
                  <text variable="section"/>
                </group>
              </else-if>
              <else>
                <text variable="section"/>
              </else>
            </choose>
          </group>
        </if>
        <else-if variable="genre" match="any">
          <choose>
            <if variable="locator">
              <text variable="locator"/>
            </if>
            <else>
              <text variable="section"/>
            </else>
          </choose>
        </else-if>
        <else>
          <choose>
            <if variable="locator">
              <text variable="locator"/>
            </if>
            <else>
              <group delimiter=" ">
                <label variable="section" form="symbol"/>
                <text variable="section"/>
              </group>
            </else>
          </choose>
        </else>
      </choose>
    </group>
  </macro>

  <macro name="tx-publication-parenthetical">
    <choose>
      <if variable="edition publisher publisher-place issued status medium note" match="any">
        <group prefix="(" suffix=")" delimiter="; ">
          <choose>
            <if variable="edition">
              <group delimiter=" ">
                <number variable="edition" form="ordinal"/>
                <text term="edition" form="short"/>
              </group>
            </if>
          </choose>
          <choose>
            <if variable="publisher publisher-place note issued status medium" match="any">
              <group delimiter=" ">
                <choose>
                  <if variable="publisher publisher-place" match="any">
                    <group delimiter=", ">
                      <text variable="publisher"/>
                      <text variable="publisher-place"/>
                    </group>
                  </if>
                </choose>
                <choose>
                  <if variable="note">
                    <text variable="note"/>
                  </if>
                  <else-if variable="issued">
                    <date variable="issued">
                      <date-part name="year"/>
                    </date>
                  </else-if>
                  <else-if variable="status">
                    <text variable="status"/>
                  </else-if>
                  <else-if variable="medium">
                    <text variable="medium"/>
                  </else-if>
                </choose>
              </group>
            </if>
          </choose>
        </group>
      </if>
    </choose>
  </macro>

  <macro name="tx-authority-status-parenthetical">
    <choose>
      <if variable="authority status issued volume container-title page note" match="any">
        <group prefix="(" suffix=")" delimiter="; ">
          <choose>
            <if variable="authority status issued" match="any">
              <group delimiter=", ">
                <text variable="authority"/>
                <choose>
                  <if variable="status">
                    <text variable="status"/>
                  </if>
                </choose>
                <choose>
                  <if variable="issued">
                    <date variable="issued">
                      <date-part name="year"/>
                    </date>
                  </if>
                </choose>
              </group>
            </if>
          </choose>
          <choose>
            <if variable="volume container-title page" match="all">
              <choose>
                <if type="regulation" match="none">
                  <group delimiter=" ">
                    <text variable="volume"/>
                    <text variable="container-title"/>
                    <text variable="page"/>
                  </group>
                </if>
                <else-if type="regulation" match="any">
                  <if variable="section">
                    <group delimiter=" ">
                      <text variable="volume"/>
                      <text variable="container-title"/>
                      <text variable="page"/>
                    </group>
                  </if>
                </else-if>
              </choose>
            </if>
          </choose>
          <choose>
            <if variable="note">
              <text variable="note"/>
            </if>
          </choose>
        </group>
      </if>
    </choose>
  </macro>

  <macro name="tx-session-law-citation">
    <group delimiter=", ">
      <text variable="title"/>
      <text variable="collection-number"/>
      <choose>
        <if variable="number section" match="any">
          <group delimiter=", ">
            <choose>
              <if variable="number">
                <text variable="number" prefix="ch. "/>
              </if>
            </choose>
            <choose>
              <if variable="section">
                <group delimiter=" ">
                  <text term="section" form="symbol"/>
                  <text variable="section"/>
                </group>
              </if>
            </choose>
          </group>
        </if>
      </choose>
      <choose>
        <if variable="volume container-title page" match="any">
          <group delimiter=" ">
            <text variable="volume"/>
            <text variable="container-title"/>
            <text variable="page"/>
          </group>
        </if>
      </choose>
      <text variable="locator"/>
    </group>
    <choose>
      <if variable="references">
        <text variable="references" prefix=", "/>
      </if>
    </choose>
  </macro>

  <macro name="tex-code-section-short">
    <group delimiter=" ">
      <choose>
        <if variable="container-title-short">
          <text variable="container-title-short"/>
        </if>
        <else>
          <text variable="container-title"/>
        </else>
      </choose>
      <choose>
        <if variable="chapter-number">
          <group delimiter=" ">
            <text variable="chapter-number"/>
            <choose>
              <if variable="locator">
                <text macro="tex-labeled-locator"/>
              </if>
              <else-if type="regulation">
                <group delimiter=" ">
                  <text term="section" form="symbol"/>
                  <text variable="section"/>
                </group>
              </else-if>
              <else>
                <text variable="section"/>
              </else>
            </choose>
          </group>
        </if>
        <else-if variable="genre" match="any">
          <choose>
            <if variable="locator">
              <text variable="locator"/>
            </if>
            <else>
              <text variable="section"/>
            </else>
          </choose>
        </else-if>
        <else>
          <choose>
            <if variable="locator">
              <text variable="locator"/>
            </if>
            <else>
              <group delimiter=" ">
                <label variable="section" form="symbol"/>
                <text variable="section"/>
              </group>
            </else>
          </choose>
        </else>
      </choose>
    </group>
  </macro>

  <!-- TOA: bibliography runs lack note numbers, so guard on first-reference-note-number
       to keep first-cite routing in place without disrupting note-mode short forms. -->
  <macro name="tex-statute">
    <choose>
      <if variable="first-reference-note-number" match="none">
        <text macro="tex-statute-first"/>
      </if>
      <else-if position="first">
        <text macro="tex-statute-first"/>
      </else-if>
      <else-if variable="references">
        <text macro="tex-statute-cross-reference"/>
      </else-if>
      <else>
        <text macro="tex-statute-short"/>
      </else>
    </choose>
  </macro>

  <macro name="tex-statute-first">
    <group delimiter=" ">
      <text macro="tex-code-section"/>
      <choose>
        <if variable="genre">
          <text/>
        </if>
        <else>
          <text macro="tx-publication-parenthetical"/>
        </else>
      </choose>
    </group>
  </macro>

  <macro name="tex-statute-short">
    <text macro="tex-short-form-base"/>
  </macro>

  <macro name="tex-statute-cross-reference">
    <group delimiter=" ">
      <text macro="cross-reference-cue"/>
      <group delimiter=", ">
        <text macro="tex-short-form-cross-base"/>
        <text variable="references"/>
      </group>
    </group>
  </macro>

  <macro name="tex-rule">
    <choose>
      <if variable="first-reference-note-number" match="none">
        <text macro="tex-rule-first"/>
      </if>
      <else-if position="first">
        <text macro="tex-rule-first"/>
      </else-if>
      <else-if variable="references">
        <text macro="tex-rule-cross-reference"/>
      </else-if>
      <else>
        <text macro="tex-rule-short"/>
      </else>
    </choose>
  </macro>

  <macro name="tex-rule-first">
    <group delimiter=" ">
      <text macro="tex-code-section"/>
      <text macro="tx-publication-parenthetical"/>
    </group>
  </macro>

  <macro name="tex-rule-short">
    <text macro="tex-short-form-base"/>
  </macro>

  <macro name="tex-rule-cross-reference">
    <group delimiter=" ">
      <text macro="cross-reference-cue"/>
      <group delimiter=", ">
        <text macro="tex-short-form-cross-base"/>
        <text variable="references"/>
      </group>
    </group>
  </macro>
  <macro name="constitution-core">
    <group delimiter=", ">
      <group delimiter=" ">
        <text variable="container-title"/>
        <group delimiter=" "><text term="article" form="short"/><text variable="chapter-number"/></group>
      </group>
      <group delimiter=" "><text term="section" form="symbol"/><text variable="section"/></group>
    </group>
    <text variable="status" prefix=" (" suffix=")"/>
  </macro>
  <macro name="tex-code-section">
    <group delimiter=" ">
      <text variable="container-title"/>
      <choose>
        <if variable="chapter-number">
          <group delimiter=" ">
            <text variable="chapter-number"/>
            <choose>
              <if type="regulation">
                <group delimiter=" "><text term="section" form="symbol"/><text variable="section"/></group>
              </if>
              <else>
                <text variable="section"/>
              </else>
            </choose>
          </group>
        </if>
        <else-if variable="genre" match="any">
          <text variable="section"/>
        </else-if>
        <else>
          <group delimiter=" "><label variable="section" form="symbol"/><text variable="section"/></group>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="tex-admin-core">
    <choose>
      <if variable="section">
        <group delimiter=" ">
          <text variable="volume"/>
          <text variable="container-title"/>
          <group delimiter=" "><text term="section" form="symbol"/><text variable="section"/></group>
        </group>
      </if>
      <else>
        <group delimiter=" "><text variable="volume"/><text variable="container-title"/><text variable="page"/></group>
      </else>
    </choose>
  </macro>
  <macro name="tex-admin-code">
    <choose>
      <if variable="first-reference-note-number" match="none">
        <text macro="tex-admin-code-first"/>
      </if>
      <else-if position="first">
        <text macro="tex-admin-code-first"/>
      </else-if>
      <else-if variable="references">
        <text macro="tex-admin-code-cross-reference"/>
      </else-if>
      <else>
        <text macro="tex-admin-code-short"/>
      </else>
    </choose>
  </macro>

  <macro name="tex-admin-code-first">
    <group delimiter=" ">
      <text macro="tex-admin-core"/>
      <text macro="tx-authority-status-parenthetical"/>
    </group>
  </macro>

  <macro name="tex-admin-code-short-base">
    <text macro="tex-admin-core"/>
  </macro>

  <macro name="tex-admin-code-short-string">
    <group delimiter=" ">
      <text macro="tex-admin-code-short-base"/>
      <choose>
        <if variable="note">
          <text variable="note" prefix="(" suffix=")"/>
        </if>
      </choose>
      <choose>
        <if variable="authority">
          <text variable="authority" prefix="(" suffix=")"/>
        </if>
      </choose>
    </group>
  </macro>

  <macro name="tex-short-form-base">
    <choose>
      <if type="regulation">
        <text macro="tex-admin-code-short-string"/>
      </if>
      <else>
        <group delimiter=" ">
          <text macro="tex-code-section-short"/>
          <choose>
            <if variable="note">
              <choose>
                <if variable="genre">
                  <text/>
                </if>
                <else>
                  <text variable="note" prefix="(" suffix=")"/>
                </else>
              </choose>
            </if>
          </choose>
        </group>
      </else>
    </choose>
  </macro>

  <macro name="tex-short-form-cross-base">
    <choose>
      <if type="regulation">
        <text macro="tex-admin-code-short-string"/>
      </if>
      <else>
        <group delimiter=" ">
          <text macro="tex-code-section-short"/>
          <choose>
            <if variable="note">
              <choose>
                <if variable="genre">
                  <text/>
                </if>
                <else>
                  <text variable="note" prefix="(" suffix=")"/>
                </else>
              </choose>
            </if>
          </choose>
        </group>
      </else>
    </choose>
  </macro>

  <macro name="tex-admin-code-short">
    <text macro="tex-short-form-base"/>
  </macro>

  <macro name="tex-admin-code-cross-reference">
    <group delimiter=" ">
      <text macro="cross-reference-cue"/>
      <group delimiter=", ">
        <text macro="tex-short-form-cross-base"/>
        <text variable="references"/>
      </group>
    </group>
  </macro>

  <macro name="book-entry">
    <group delimiter=", ">
      <names variable="author">
        <name and="text" delimiter=", "/>
      </names>
      <group delimiter=" ">
        <number variable="volume" form="numeric"/>
        <text variable="title" font-style="italic"/>
      </group>
      <text macro="tex-labeled-locator"/>
      <date variable="issued">
        <date-part name="year"/>
      </date>
    </group>
  </macro>
  <macro name="municipal-code">
    <group delimiter=", ">
      <text variable="authority"/>
      <text macro="tex-code-section"/>
    </group>
    <date variable="issued" prefix=" (" suffix=")"><date-part name="year"/></date>
  </macro>
  <macro name="ag-opinion">
    <group delimiter=" "><text variable="authority"/><text variable="number" prefix="No. "/></group>
    <date variable="issued" prefix=" (" suffix=")"><date-part name="year"/></date>
  </macro>

  <!-- Citation layout -->
  <citation et-al-min="4" et-al-use-first="3">
    <layout suffix=".">
      <choose>
        <if type="legal_case"><text macro="legal-case"/></if>
        <else-if type="regulation">
          <choose>
            <if variable="volume"><text macro="tex-admin-code"/></if>
            <else-if variable="authority chapter-number" match="all"><text macro="municipal-code"/></else-if>
            <else><text macro="tex-rule"/></else>
          </choose>
        </else-if>
        <else-if type="legislation">
          <choose>
            <if variable="title"><text macro="tx-session-law-citation"/></if>
            <else-if variable="chapter-number"><text macro="constitution-core"/></else-if>
            <else-if variable="genre" match="any"><text macro="tex-rule"/></else-if>
            <else><text macro="tex-statute"/></else>
          </choose>
        </else-if>
        <else-if type="report"><text macro="ag-opinion"/></else-if>
        <else-if type="book"><text macro="book-entry"/></else-if>
        <else><text variable="title"/></else>
      </choose>
    </layout>
  </citation>

  <!-- Bibliography grouped order via macro -->
  <bibliography hanging-indent="false" second-field-align="flush">
    <sort>
      <key macro="toa-type-rank"/>
      <key variable="authority"/>
      <key variable="container-title"/>
      <key variable="volume"/>
      <key variable="page"/>
      <key variable="title"/>
    </sort>
    <layout suffix=".">
      <choose>
        <if type="legal_case"><text macro="legal-case"/></if>
        <else-if type="regulation">
          <choose>
            <if variable="volume"><text macro="tex-admin-code"/></if>
            <else-if variable="authority chapter-number" match="all"><text macro="municipal-code"/></else-if>
            <else><text macro="tex-rule"/></else>
          </choose>
        </else-if>
        <else-if type="legislation">
          <choose>
            <if variable="title"><text macro="tx-session-law-citation"/></if>
            <else-if variable="chapter-number"><text macro="constitution-core"/></else-if>
            <else-if variable="genre" match="any"><text macro="tex-rule"/></else-if>
            <else><text macro="tex-statute"/></else>
          </choose>
        </else-if>
        <else-if type="report"><text macro="ag-opinion"/></else-if>
        <else-if type="book"><text macro="book-entry"/></else-if>
        <else><text variable="title"/></else>
      </choose>
    </layout>
  </bibliography>
</style>
