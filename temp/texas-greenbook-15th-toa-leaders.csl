<?xml version='1.0' encoding='UTF-8'?>
<ns0:style xmlns:ns0="http://purl.org/net/xbiblio/csl" version="1.0.2" class="note" default-locale="en-US">
  <ns0:info>
    <ns0:title>Texas Greenbook (15th ed.) — Table of Authorities (Leaders)</ns0:title>
    <ns0:title-short>Texas Greenbook 15th TOA (Leaders)</ns0:title-short>
    <ns0:id>http://www.zotero.org/styles/texas-greenbook-15th-toa-leaders</ns0:id>
    <ns0:link href="http://www.zotero.org/styles/texas-greenbook-15th-toa" rel="self" />
    <ns0:link href="http://www.zotero.org/styles/texas-greenbook-15th-master" rel="template" />
    <ns0:link href="https://www.txcourts.gov/rules-forms/rules-standards/" rel="documentation" />
    <ns0:author><ns0:name>GPT-5 Pro</ns0:name></ns0:author>
    <ns0:category citation-format="note" />
    <ns0:category field="law" />
    <ns0:updated>2025-11-01T00:53:48Z</ns0:updated>
    <ns0:summary>TOA-style bibliography for Texas practice: prints full case and authority cites, sorts by authority, and minimizes styling for easy copy into a Table of Authorities.</ns0:summary>
  </ns0:info>

  <ns0:locale xml:lang="en-US">
    <ns0:terms>
      <ns0:term name="section" form="symbol">§</ns0:term>
      <ns0:term name="article" form="short">art.</ns0:term>
      <ns0:term name="chapter" form="short">ch.</ns0:term>
      <ns0:term name="rule" form="short">R.</ns0:term>
    </ns0:terms>
  </ns0:locale>

  
  <ns0:macro name="case-name"><ns0:text variable="title" font-style="italic" /></ns0:macro>
  <ns0:macro name="reporter-print">
    <ns0:group delimiter=" ">
      <ns0:text variable="volume" />
      <ns0:text variable="container-title" />
      <ns0:text variable="page" />
    </ns0:group>
  </ns0:macro>
  <ns0:macro name="parenthetical-docket">
    <ns0:choose>
      <ns0:if variable="number collection-number" match="any">
        <ns0:group delimiter=", ">
          <ns0:text variable="number" prefix="No. " />
          <ns0:text variable="collection-number" />
        </ns0:group>
      </ns0:if>
    </ns0:choose>
  </ns0:macro>
  <ns0:macro name="reporter-wl">
    <ns0:text macro="parenthetical-docket" />
  </ns0:macro>
  <ns0:macro name="pinpoint">
    <ns0:choose>
      <ns0:if variable="volume page container-title" match="all"><ns0:text variable="locator" prefix=", " /></ns0:if>
      <ns0:else-if variable="collection-number locator" match="all"><ns0:text variable="locator" prefix=", at *" /></ns0:else-if>
    </ns0:choose>
  </ns0:macro>

  <ns0:macro name="tex-labeled-locator">
    <ns0:choose>
      <ns0:if locator="section paragraph" match="any">
        <ns0:group delimiter=" "><ns0:label variable="locator" form="symbol" /><ns0:text variable="locator" /></ns0:group>
      </ns0:if>
      <ns0:else-if locator="rule regulation" match="any">
        <ns0:group delimiter=" "><ns0:label variable="locator" form="short" /><ns0:text variable="locator" /></ns0:group>
      </ns0:else-if>
      <ns0:else-if locator="article chapter part title volume" match="any">
        <ns0:group delimiter=" "><ns0:label variable="locator" form="short" /><ns0:text variable="locator" /></ns0:group>
      </ns0:else-if>
      <ns0:else>
        <ns0:text variable="locator" />
      </ns0:else>
    </ns0:choose>
  </ns0:macro>

  <ns0:macro name="cross-reference-cue">
    <ns0:choose>
      <ns0:if variable="annote" match="none">
        <ns0:choose>
          <ns0:if variable="jurisdiction">
            <ns0:text value="See also" />
          </ns0:if>
          <ns0:else>
            <ns0:text value="See" />
          </ns0:else>
        </ns0:choose>
      </ns0:if>
      <ns0:else>
        <ns0:text variable="annote" />
      </ns0:else>
    </ns0:choose>
  </ns0:macro>
  <ns0:macro name="parenthetical-procedural-note">
    <ns0:choose>
      <ns0:if variable="note annote" match="all" />
      <ns0:else-if variable="note"><ns0:text variable="note" /></ns0:else-if>
    </ns0:choose>
  </ns0:macro>
  <ns0:macro name="parenthetical-procedural-status">
    <ns0:choose>
      <ns0:if variable="status note" match="any">
        <ns0:group delimiter=", ">
          <ns0:text variable="status" />
          <ns0:choose>
            <ns0:if variable="status"><ns0:text macro="parenthetical-procedural-note" /></ns0:if>
            <ns0:else-if variable="note">
              <ns0:choose>
                <ns0:if variable="references" />
                <ns0:else><ns0:text macro="parenthetical-procedural-note" /></ns0:else>
              </ns0:choose>
            </ns0:else-if>
          </ns0:choose>
        </ns0:group>
      </ns0:if>
    </ns0:choose>
  </ns0:macro>
  <ns0:macro name="court-and-date">
    <ns0:choose>
      <ns0:if variable="volume page container-title" match="all">
        <ns0:group prefix=" (" suffix=")">
          <ns0:text variable="authority" />
          <ns0:date variable="issued" prefix=" "><ns0:date-part name="year" /></ns0:date>
          <ns0:text macro="parenthetical-procedural-status" prefix=", " />
        </ns0:group>
      </ns0:if>
      <ns0:else>
        <ns0:group prefix=" (" suffix=")">
          <ns0:text variable="authority" />
          <ns0:date variable="issued" prefix=" ">
            <ns0:date-part name="month" form="short" suffix=" " />
            <ns0:date-part name="day" form="numeric" suffix=", " />
            <ns0:date-part name="year" />
          </ns0:date>
          <ns0:text macro="parenthetical-procedural-status" prefix=", " />
        </ns0:group>
      </ns0:else>
    </ns0:choose>
  </ns0:macro>
  <ns0:macro name="parenthetical-slip-op">
    <ns0:choose>
      <ns0:if variable="genre medium" match="any">
        <ns0:group delimiter="">
          <ns0:text variable="genre" font-style="normal" prefix=" (" suffix=")" />
          <ns0:text variable="medium" font-style="normal" prefix=" (" suffix=")" />
        </ns0:group>
      </ns0:if>
    </ns0:choose>
  </ns0:macro>
  <ns0:macro name="explanatory-parenthetical">
    <ns0:choose>
      <ns0:if variable="abstract"><ns0:text variable="abstract" prefix=" (" suffix=")" /></ns0:if>
    </ns0:choose>
  </ns0:macro>
  <ns0:macro name="case-parenthetical-stack">
    <ns0:text macro="parenthetical-slip-op" />
    <ns0:text macro="explanatory-parenthetical" />
  </ns0:macro>
  <ns0:macro name="toa-type-rank">
    <ns0:choose>
      <ns0:if variable="keyword">
        <ns0:text variable="keyword" />
      </ns0:if>
      <ns0:else>
        <ns0:choose>
          <ns0:if type="legal_case">
            <ns0:text value="01" />
          </ns0:if>
          <ns0:else-if type="legislation">
            <ns0:choose>
              <ns0:if variable="chapter-number">
                <ns0:text value="02" />
              </ns0:if>
              <ns0:else-if variable="title">
                <ns0:text value="06" />
              </ns0:else-if>
              <ns0:else>
                <ns0:text value="03" />
              </ns0:else>
            </ns0:choose>
          </ns0:else-if>
          <ns0:else-if type="regulation">
            <ns0:choose>
              <ns0:if variable="volume">
                <ns0:text value="04" />
              </ns0:if>
              <ns0:else-if variable="authority chapter-number" match="all">
                <ns0:text value="08" />
              </ns0:else-if>
              <ns0:else>
                <ns0:text value="05" />
              </ns0:else>
            </ns0:choose>
          </ns0:else-if>
          <ns0:else-if type="report">
            <ns0:text value="07" />
          </ns0:else-if>
          <ns0:else>
            <ns0:text value="09" />
          </ns0:else>
        </ns0:choose>
      </ns0:else>
    </ns0:choose>
  </ns0:macro>

  <ns0:macro name="toa-section-heading">
    <ns0:choose>
      <ns0:if variable="call-number">
        <ns0:text variable="call-number" />
      </ns0:if>
    </ns0:choose>
  </ns0:macro>

  <ns0:macro name="toa-subheading-heading">
    <ns0:choose>
      <ns0:if variable="reviewed-title">
        <ns0:text variable="reviewed-title" />
      </ns0:if>
    </ns0:choose>
  </ns0:macro>
  <ns0:macro name="legal-case">
    <ns0:group delimiter=", ">
      <ns0:text macro="case-name" />
      <ns0:choose>
        <ns0:if variable="volume page container-title" match="all"><ns0:text macro="reporter-print" /></ns0:if>
        <ns0:else><ns0:text macro="reporter-wl" /></ns0:else>
      </ns0:choose>
    </ns0:group>
    <ns0:text macro="pinpoint" />
    <ns0:text macro="court-and-date" />
    <ns0:text macro="case-parenthetical-stack" />
  </ns0:macro>

  <!-- Statutes, constitution, TAC, rules, session laws, municipal code, AG opinions (same as master) -->
  <ns0:macro name="tex-code-section">
    <ns0:group delimiter=" ">
      <ns0:text variable="container-title" />
      <ns0:choose>
        <ns0:if variable="chapter-number">
          <ns0:group delimiter=" ">
            <ns0:text variable="chapter-number" />
            <ns0:choose>
              <ns0:if variable="locator">
                <ns0:text macro="tex-labeled-locator" />
              </ns0:if>
              <ns0:else-if type="regulation">
                <ns0:group delimiter=" "><ns0:text term="section" form="symbol" /><ns0:text variable="section" /></ns0:group>
              </ns0:else-if>
              <ns0:else>
                <ns0:text variable="section" />
              </ns0:else>
            </ns0:choose>
          </ns0:group>
        </ns0:if>
        <ns0:else-if variable="genre" match="any">
          <ns0:choose>
            <ns0:if variable="locator">
              <ns0:text variable="locator" />
            </ns0:if>
            <ns0:else>
              <ns0:text variable="section" />
            </ns0:else>
          </ns0:choose>
        </ns0:else-if>
        <ns0:else>
          <ns0:choose>
            <ns0:if variable="locator">
              <ns0:text variable="locator" />
            </ns0:if>
            <ns0:else>
              <ns0:group delimiter=" "><ns0:label variable="section" form="symbol" /><ns0:text variable="section" /></ns0:group>
            </ns0:else>
          </ns0:choose>
        </ns0:else>
      </ns0:choose>
    </ns0:group>
  </ns0:macro>

  <ns0:macro name="tx-publication-parenthetical">
    <ns0:choose>
      <ns0:if variable="edition publisher publisher-place issued status medium note" match="any">
        <ns0:group prefix="(" suffix=")" delimiter="; ">
          <ns0:choose>
            <ns0:if variable="edition">
              <ns0:group delimiter=" ">
                <ns0:number variable="edition" form="ordinal" />
                <ns0:text term="edition" form="short" />
              </ns0:group>
            </ns0:if>
          </ns0:choose>
          <ns0:choose>
            <ns0:if variable="publisher publisher-place note issued status medium" match="any">
              <ns0:group delimiter=" ">
                <ns0:choose>
                  <ns0:if variable="publisher publisher-place" match="any">
                    <ns0:group delimiter=", ">
                      <ns0:text variable="publisher" />
                      <ns0:text variable="publisher-place" />
                    </ns0:group>
                  </ns0:if>
                </ns0:choose>
                <ns0:choose>
                  <ns0:if variable="note">
                    <ns0:text variable="note" />
                  </ns0:if>
                  <ns0:else-if variable="issued">
                    <ns0:date variable="issued"><ns0:date-part name="year" /></ns0:date>
                  </ns0:else-if>
                  <ns0:else-if variable="status">
                    <ns0:text variable="status" />
                  </ns0:else-if>
                  <ns0:else-if variable="medium">
                    <ns0:text variable="medium" />
                  </ns0:else-if>
                </ns0:choose>
              </ns0:group>
            </ns0:if>
          </ns0:choose>
        </ns0:group>
      </ns0:if>
    </ns0:choose>
  </ns0:macro>

  <ns0:macro name="tx-authority-status-parenthetical">
    <ns0:choose>
      <ns0:if variable="authority status issued volume container-title page note" match="any">
        <ns0:group prefix="(" suffix=")" delimiter="; ">
          <ns0:choose>
            <ns0:if variable="authority status issued" match="any">
              <ns0:group delimiter=", ">
                <ns0:text variable="authority" />
                <ns0:choose>
                  <ns0:if variable="status">
                    <ns0:text variable="status" />
                  </ns0:if>
                </ns0:choose>
                <ns0:choose>
                  <ns0:if variable="issued">
                    <ns0:date variable="issued"><ns0:date-part name="year" /></ns0:date>
                  </ns0:if>
                </ns0:choose>
              </ns0:group>
            </ns0:if>
          </ns0:choose>
          <ns0:choose>
            <ns0:if variable="volume container-title page" match="all">
              <ns0:choose>
                <ns0:if type="regulation" match="none">
                  <ns0:group delimiter=" ">
                    <ns0:text variable="volume" />
                    <ns0:text variable="container-title" />
                    <ns0:text variable="page" />
                  </ns0:group>
                </ns0:if>
                <ns0:else-if type="regulation" match="any">
                  <ns0:if variable="section">
                    <ns0:group delimiter=" ">
                      <ns0:text variable="volume" />
                      <ns0:text variable="container-title" />
                      <ns0:text variable="page" />
                    </ns0:group>
                  </ns0:if>
                </ns0:else-if>
              </ns0:choose>
            </ns0:if>
          </ns0:choose>
          <ns0:choose>
            <ns0:if variable="note">
              <ns0:text variable="note" />
            </ns0:if>
          </ns0:choose>
        </ns0:group>
      </ns0:if>
    </ns0:choose>
  </ns0:macro>

  <ns0:macro name="tx-session-law-citation">
    <ns0:group delimiter=", ">
      <ns0:text variable="title" />
      <ns0:text variable="collection-number" />
      <ns0:choose>
        <ns0:if variable="number section" match="any">
          <ns0:group delimiter=", ">
            <ns0:choose>
              <ns0:if variable="number">
                <ns0:text variable="number" prefix="ch. " />
              </ns0:if>
            </ns0:choose>
            <ns0:choose>
              <ns0:if variable="section">
                <ns0:group delimiter=" ">
                  <ns0:text term="section" form="symbol" />
                  <ns0:text variable="section" />
                </ns0:group>
              </ns0:if>
            </ns0:choose>
          </ns0:group>
        </ns0:if>
      </ns0:choose>
      <ns0:choose>
        <ns0:if variable="volume container-title page" match="any">
          <ns0:group delimiter=" ">
            <ns0:text variable="volume" />
            <ns0:text variable="container-title" />
            <ns0:text variable="page" />
          </ns0:group>
        </ns0:if>
      </ns0:choose>
      <ns0:text variable="locator" />
    </ns0:group>
    <ns0:choose>
      <ns0:if variable="references">
        <ns0:text variable="references" prefix=", " />
      </ns0:if>
    </ns0:choose>
  </ns0:macro>

  <ns0:macro name="tex-code-section-short">
    <ns0:group delimiter=" ">
      <ns0:choose>
        <ns0:if variable="container-title-short">
          <ns0:text variable="container-title-short" />
        </ns0:if>
        <ns0:else>
          <ns0:text variable="container-title" />
        </ns0:else>
      </ns0:choose>
      <ns0:choose>
        <ns0:if variable="chapter-number">
          <ns0:group delimiter=" ">
            <ns0:text variable="chapter-number" />
            <ns0:choose>
              <ns0:if type="regulation">
                <ns0:group delimiter=" "><ns0:text term="section" form="symbol" /><ns0:text variable="section" /></ns0:group>
              </ns0:if>
              <ns0:else>
                <ns0:text variable="section" />
              </ns0:else>
            </ns0:choose>
          </ns0:group>
        </ns0:if>
        <ns0:else-if variable="genre" match="any">
          <ns0:text variable="section" />
        </ns0:else-if>
        <ns0:else>
          <ns0:group delimiter=" "><ns0:label variable="section" form="symbol" /><ns0:text variable="section" /></ns0:group>
        </ns0:else>
      </ns0:choose>
    </ns0:group>
  </ns0:macro>

  <!-- TOA: bibliography runs lack note numbers, so guard on first-reference-note-number
       to keep first-cite routing in place without disrupting note-mode short forms. -->
  <ns0:macro name="tex-statute">
    <ns0:choose>
      <ns0:if variable="first-reference-note-number" match="none">
        <ns0:text macro="tex-statute-first" />
      </ns0:if>
      <ns0:else-if position="first">
        <ns0:text macro="tex-statute-first" />
      </ns0:else-if>
      <ns0:else-if variable="references">
        <ns0:text macro="tex-statute-cross-reference" />
      </ns0:else-if>
      <ns0:else>
        <ns0:text macro="tex-statute-short" />
      </ns0:else>
    </ns0:choose>
  </ns0:macro>

  <ns0:macro name="tex-statute-first">
    <ns0:group delimiter=" ">
      <ns0:text macro="tex-code-section" />
      <ns0:choose>
        <ns0:if variable="genre">
          <ns0:text />
        </ns0:if>
        <ns0:else>
          <ns0:text macro="tx-publication-parenthetical" />
        </ns0:else>
      </ns0:choose>
    </ns0:group>
  </ns0:macro>

  <ns0:macro name="tex-statute-short">
    <ns0:text macro="tex-short-form-base" />
  </ns0:macro>

  <ns0:macro name="tex-statute-cross-reference">
    <ns0:group delimiter=" ">
      <ns0:text macro="cross-reference-cue" />
      <ns0:group delimiter=", ">
        <ns0:text macro="tex-short-form-cross-base" />
        <ns0:text variable="references" />
      </ns0:group>
    </ns0:group>
  </ns0:macro>

  <ns0:macro name="tex-rule">
    <ns0:choose>
      <ns0:if variable="first-reference-note-number" match="none">
        <ns0:text macro="tex-rule-first" />
      </ns0:if>
      <ns0:else-if position="first">
        <ns0:text macro="tex-rule-first" />
      </ns0:else-if>
      <ns0:else-if variable="references">
        <ns0:text macro="tex-rule-cross-reference" />
      </ns0:else-if>
      <ns0:else>
        <ns0:text macro="tex-rule-short" />
      </ns0:else>
    </ns0:choose>
  </ns0:macro>

  <ns0:macro name="tex-rule-first">
    <ns0:group delimiter=" ">
      <ns0:text macro="tex-code-section" />
      <ns0:text macro="tx-publication-parenthetical" />
    </ns0:group>
  </ns0:macro>

  <ns0:macro name="tex-rule-short">
    <ns0:text macro="tex-short-form-base" />
  </ns0:macro>

  <ns0:macro name="tex-rule-cross-reference">
    <ns0:group delimiter=" ">
      <ns0:text macro="cross-reference-cue" />
      <ns0:group delimiter=", ">
        <ns0:text macro="tex-short-form-cross-base" />
        <ns0:text variable="references" />
      </ns0:group>
    </ns0:group>
  </ns0:macro>
  <ns0:macro name="constitution-core">
    <ns0:group delimiter=", ">
      <ns0:group delimiter=" ">
        <ns0:text variable="container-title" />
        <ns0:group delimiter=" "><ns0:text term="article" form="short" /><ns0:text variable="chapter-number" /></ns0:group>
      </ns0:group>
      <ns0:group delimiter=" "><ns0:text term="section" form="symbol" /><ns0:text variable="section" /></ns0:group>
    </ns0:group>
    <ns0:text variable="status" prefix=" (" suffix=")" />
  </ns0:macro>
  <ns0:macro name="tex-code-section">
    <ns0:group delimiter=" ">
      <ns0:text variable="container-title" />
      <ns0:choose>
        <ns0:if variable="chapter-number">
          <ns0:group delimiter=" ">
            <ns0:text variable="chapter-number" />
            <ns0:choose>
              <ns0:if variable="locator">
                <ns0:text macro="tex-labeled-locator" />
              </ns0:if>
              <ns0:else-if type="regulation">
                <ns0:group delimiter=" "><ns0:text term="section" form="symbol" /><ns0:text variable="section" /></ns0:group>
              </ns0:else-if>
              <ns0:else><ns0:text variable="section" /></ns0:else>
            </ns0:choose>
          </ns0:group>
        </ns0:if>
        <ns0:else-if variable="genre" match="any">
          <ns0:choose>
            <ns0:if variable="locator">
              <ns0:text variable="locator" />
            </ns0:if>
            <ns0:else><ns0:text variable="section" /></ns0:else>
          </ns0:choose>
        </ns0:else-if>
        <ns0:else>
          <ns0:choose>
            <ns0:if variable="locator">
              <ns0:text variable="locator" />
            </ns0:if>
            <ns0:else><ns0:group delimiter=" "><ns0:label variable="section" form="symbol" /><ns0:text variable="section" /></ns0:group></ns0:else>
          </ns0:choose>
        </ns0:else>
      </ns0:choose>
    </ns0:group>
  </ns0:macro>
  <ns0:macro name="tex-admin-core">
    <ns0:choose>
      <ns0:if variable="section">
        <ns0:group delimiter=" ">
          <ns0:text variable="volume" />
          <ns0:text variable="container-title" />
          <ns0:group delimiter=" "><ns0:text term="section" form="symbol" /><ns0:text variable="section" /></ns0:group>
        </ns0:group>
      </ns0:if>
      <ns0:else>
        <ns0:group delimiter=" "><ns0:text variable="volume" /><ns0:text variable="container-title" /><ns0:text variable="page" /></ns0:group>
      </ns0:else>
    </ns0:choose>
  </ns0:macro>
  <ns0:macro name="tex-admin-code">
    <ns0:choose>
      <ns0:if variable="first-reference-note-number" match="none">
        <ns0:text macro="tex-admin-code-first" />
      </ns0:if>
      <ns0:else-if position="first">
        <ns0:text macro="tex-admin-code-first" />
      </ns0:else-if>
      <ns0:else-if variable="references">
        <ns0:text macro="tex-admin-code-cross-reference" />
      </ns0:else-if>
      <ns0:else>
        <ns0:text macro="tex-admin-code-short" />
      </ns0:else>
    </ns0:choose>
  </ns0:macro>

  <ns0:macro name="tex-admin-code-first">
    <ns0:group delimiter=" ">
      <ns0:text macro="tex-admin-core" />
